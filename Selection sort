class Solution
{
    public:
    int select(int arr[], int i)
    {
        
    }
     
    void selectionSort(int a[], int n)
    {
       for(int i =0;i<n;i++){
           for(int j =i+1 ;j<n;j++){
               if(a[i]>a[j]){
                   swap(a[i],a[j]);
               }
           }
       }
       
    }
};
